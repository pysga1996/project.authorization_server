<component name="ProjectRunConfigurationManager">
  <configuration default="false" factoryName="Spring Boot"
    name="LAMBDA_AUTH_SERVICE" type="SpringBootApplicationConfigurationType">
    <extension name="net.ashald.envfile">
      <ENTRIES>
        <ENTRY IS_ENABLED="true" PARSER="runconfig"/>
        <ENTRY IS_ENABLED="true" PARSER="env"
          PATH="$PROJECT_DIR$/../../../../CodeGym/Module 4/Project Climax Sound/lambda-auth-service/environment/.env"/>
      </ENTRIES>
      <option name="IS_SUBST" value="true"/>
      <option name="IS_PATH_MACRO_SUPPORTED" value="true"/>
      <option name="IS_IGNORE_MISSING_FILES" value="false"/>
      <option name="IS_ENABLE_EXPERIMENTAL_INTEGRATIONS" value="false"/>
